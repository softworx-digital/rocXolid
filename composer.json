{
    "name": "softworx/rocxolid",
    "description": "Laravel CRUD & components library and extension",
    "keywords": [
        "laravel",
        "crud"
    ],
    "type": "library",
    "authors": [{
        "name": "softworx",
        "email": "hello@softworx.digital"
    }],
	"license": "MIT",
    "require": {
		"php": "~7.2",
        "laravel/framework": "5.8.*|^6.0",
        "laravelcollective/html": "5.8.*|^6.0",
        "barryvdh/laravel-translation-manager": "0.5.*",
        "spatie/laravel-permission": "2.*",
        "intervention/image": "2.*"
	},
    "autoload": {
        "psr-4": {
            "Softworx\\RocXolid\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softworx\\RocXolid\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    }
}